{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-undef": "off",
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off",
        "no-console": "off"
    }
}